{
    "name": "mazenovi/ldapquery",
    "type": "library",
    "description": "A light weight package for easily building LDAP advanced filter queries.",
    "keywords": [
        "active directory",
        "ldap",
        "ldapquery",
        "ldap query",
        "windows",
        "windows server",
        "ldap filters"
    ],
    "authors": [
        {
            "name": "Avadanei Danut",
            "email": "danut@worldit.info"
        }
    ],
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "illuminate/support": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3",
        "psy/psysh": "^0.7.2",
        "symfony/ldap": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "LdapQuery\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LdapQuery\\Tests\\": "tests/"
        }
    },
    "repositories":[
        {
            "packagist": false
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ]
}
